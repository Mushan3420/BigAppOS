<resources>

    <style name="TV_version_code">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#303030</item>
    </style>

    <style name="TV_left_menu">
        <item name="android:paddingTop">17dp</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/v_tv_left_menu</item>
        <item name="android:drawablePadding">30dp</item>
    </style>

    <style name="TV_topic_item_title">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">2</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">?attr/c_txt_home_item_title</item>
    </style>

    <style name="TV_topic_item_title_grid">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">?attr/c_txt_home_item_title</item>
    </style>

    <style name="TV_topic_item_desc_grid">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">6</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_home_item_desc</item>
    </style>

    <style name="TV_topic_item_time">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/c_txt_home_item_time</item>
    </style>

    <style name="TV_topic_item_desc">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">2</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_home_item_desc</item>
    </style>

    <style name="TV_topic_item_desc_single">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#999999</item>
    </style>

    <style name="TV_topic_item_comment_num">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/c_txt_item_comment_num</item>
    </style>

    <style name="TV_search_history_item">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_name</item>
    </style>

    <style name="TV_search_history">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_desc</item>
    </style>

    <style name="TV_history_read_item">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_name</item>
    </style>

    <style name="TV_history_read_item_time">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_time</item>
    </style>

    <style name="TV_commit_my_name">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#303030</item>
    </style>

    <style name="TV_commit_my_content">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
        <item name="android:maxLines">100</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_desc</item>
    </style>

    <style name="TV_commit_name">
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_name</item>
    </style>

    <style name="TV_commit_content">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_desc</item>
    </style>

    <style name="TV_desc_tags">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_desc</item>
    </style>

    <style name="TV_commit_time">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_time</item>
    </style>

    <style name="TV_commit_my_title">
        <item name="android:ellipsize">middle</item>
        <item name="android:background">?attr/img_shape_commit_my_title</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_name</item>
    </style>

    <style name="TV_left_logout">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:paddingTop">17dp</item>
        <item name="android:paddingBottom">17dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/z_item_bg</item>
    </style>

    <style name="TV_go_regist">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/v_tv_go_regist</item>
    </style>

    <style name="ET_login">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#303030</item>
        <item name="android:textColorHint">#E6E6E6</item>
    </style>

    <style name="ET_comment_nobody">
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/c_txt_topic_comment_item_name</item>
        <item name="android:textColorHint">?attr/c_txt_topic_comment_item_time</item>
    </style>
</resources>
